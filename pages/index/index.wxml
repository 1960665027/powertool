<view class="zhanshi">
  {{jieguo}}Ω ±{{wucha}}%</view>
<canvas canvas-id="myCanvas" style="height:70rpx;" />
<view class="zhengti">
  <view class="diyihua">
    <text class="diyititle">第一环</text>
    <radio-group class="radio-group" bindchange="radioChange1">
      <label style="display:flex" class="radio" wx:for="{{items1}}">
        <radio  color="{{item.color}}" value="{{item.name}}" checked="{{item.checked}}" />{{item.value}}
        <view class="diyihuan" style="background:{{item.color}}"></view>
      </label>
    </radio-group>
  </view>
